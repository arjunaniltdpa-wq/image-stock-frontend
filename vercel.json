{
  "version": 2,

  "routes": [
    {
      "src": "^/photo/.*",
      "dest": "/api/og"
    }
  ],

  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|webp|svg|gif|css|js|woff|woff2|ttf)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
